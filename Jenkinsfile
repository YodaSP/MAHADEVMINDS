                echo 'Git checkout'
            }
        }
         stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
      stage('Deploy') {
            steps {
                echo 'Deployment'
            }
        }
    }
}
